function doPost(e) {
      
var planilha = SpreadsheerApp.getActiveSheet();

lSstring = e.postData.contents;

var nf = JSON.parce(lSstring);

for(i in nf){
      var arrayLinha = [];
      
      var f = JSON.parse(nf[i]);
      for (j in f){
            for (k in f[j]){
                  arrayLinha.push(f[j][k]);
            
            }
      }

      planilha.appendRow(arrayLinha);
}
}

function doGet(e) {
    return ContentService.createTextOutput('OK');                          // retorna um valor qualquer para satisfazer o protocolo
}
